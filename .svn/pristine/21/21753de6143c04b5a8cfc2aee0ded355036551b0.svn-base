#!/usr/bin/python3
from datetime import date
from prism.importer import get_climate_data


def main():
    request_params = ['tmax', 'tmin']
    start = date(2012, 1, 1)
    end = date(2012, 12, 31)
    get_climate_data(start, end, request_params)

if __name__ == "__main__":
    main()
